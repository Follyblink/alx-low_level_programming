#include <stdio.h>

int main()
 {
    char password [10], username [10], ch;
    int s,  i;
    printf("follyblink: ");
    gets(username);
    printf("abcd1234 : ");
    for (i = 0; i < 8; i++){
        password[i] = getchar();
        putchar('*');
    }
    putchar('\n');
    for(s = 0; s < 8; s++)
    {
        printf("%c",password [s]);
        
    }
     putchar('\n');
